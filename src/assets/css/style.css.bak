}

form label {
    font-family: Space Grotesk;
    font-size: 14px;
    font-weight: 700;
    line-height: 18px;
    letter-spacing: 0.1em;
    text-align: left;
    color: #5272B0;
    /* opacity: 50%; */
}

.calebdar select.form-control {
    width: auto;
    width: 167px;
}

