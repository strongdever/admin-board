*,
body {
    font-family: 'Space Grotesk', sans-serif;
    font-weight: 500;
}

img.full-width {
    width: 100%;
}

.cursor {
    cursor: pointer;
}

.sidebar-brand-text img.full-width {
    border-bottom: rgba(255, 255, 255, .15) solid 3px;
    padding-bottom: 14px;
}

.sidebar li.nav-item a {
    font-family: Space Grotesk;
    font-size: 16px;
    font-weight: 700;
    line-height: 20px;
    padding: 0px;
    letter-spacing: 0.1em;
    text-align: left;
    text-transform: uppercase;
    padding: 0px 0px;
}

.sidebar li.nav-item a {
    border-left: 6px solid transparent;
}

.sidebar li.nav-item.active a {
    background: #ffffff21;
    color: #fff;
    border-left: 6px solid #fff;
    color: #fff;
}

.sidebar .nav-item .nav-link span {
    color: #fff;
    opacity: 1;
}

.sidebar-dark .nav-item .nav-link img {
    width: 20px;
    margin-right: 7px;
}

.sidebar-dark .nav-item.active .nav-link {
    opacity: 1;
}

.sidebar-dark .nav-item .nav-link {
    opacity: 0.6;
}

.heading h2 {
    font-family: Space Grotesk;
    font-size: 24px;
    font-weight: 700;
    line-height: 31px;
    margin-bottom: 0px;
    padding-left: 20px;
    letter-spacing: 0.1em;
    text-align: left;
    color: #5272B0;
    /* opacity: 50%; */
}

nav.navbar.navbar-expand.navbar-light.bg-white.topbar.mb-4.static-top {
    box-shadow: 0px 0px 0px 3px #5272b026;
}

a#userDropdown img {
    width: 43px;
    padding: 10px 13px;
    background: #5272B026;
    height: 42px;
    object-fit: contain;
}

#wrapper #content-wrapper {
    background-color: #5272b014!important;
}

.profile-page h3 {
    font-family: Space Grotesk;
    font-size: 18px;
    font-weight: 700;
    line-height: 23px;
    padding: 0px 0px 10px;
    color: #5272B0;
    letter-spacing: 0.1em;
    text-align: left;
    border-bottom: #5272b038 solid 2px;
    margin-bottom: 20px;
}

.shadows {
    background: #fff;
}

.main.shadows {
    padding: 17px 17px;
    box-shadow: 0px 0px 0px 2px #cccccc0f;
}

.select-files-li h4 {
    font-family: Space Grotesk;
    font-size: 14px;
    font-weight: 700;
    line-height: 18px;
    letter-spacing: 0.1em;
    text-align: left;
    color: #5272B0;
    /* opacity: 50%; */
    border-bottom: #5272b08f solid 2px;
    padding-bottom: 11px;
    margin-bottom: 0px;
    margin-top: 28px;
}

.main.shadows label {
    display: flex;
    width: 100%;
}

.main.shadows input[type="text"] {
    width: 100%;
    border: none;
    font-family: Space Grotesk;
    font-size: 14px;
    font-weight: 700;
    line-height: 18px;
    letter-spacing: 0.1em;
    padding: 15px 20px;
    text-align: left;
    line-height: normal;
    background: #5272b024;
    border-radius: unset;
}

.main.shadows input[type="datetime-local"] {
    width: 50%;
    border: none;
    font-family: Space Grotesk;
    font-size: 16px;
    font-weight: 700;
    line-height: 18px;
    letter-spacing: 0.1em;
    padding: 15px 20px;
    text-align: left;
    line-height: normal;
    background: #5272b024;
    border-radius: unset;
}

.main.shadows ::-webkit-calendar-picker-indicator {
    color: rgba(0, 0, 0, 0);
    opacity: 1;
    background-image: url('../images/calendar.png');
    width: 18px;
    cursor: pointer;
    border-radius: 50%;
    margin-left: .5rem;
}

.main.shadows button {
    border: transparent;
    /* width: 54px; */
    /* background: #5272b045; */
}

.select-files-li ul {
    list-style: none;
    padding: 0px;
}

.select-files-li ul li label {
    display: flex;
    padding: 18px 14px;
    margin-bottom: 0px!important;
}

.select-files-li ul li label input[type="radio"],
.select-files-li ul li label span {
    margin-right: 9px;
    font-family: Space Grotesk;
    font-size: 14px;
    font-weight: 700;
    line-height: 18px;
    letter-spacing: 0.1em;
    text-align: left;
    color: #4C4C4C;
    /* opacity: 50%; */
}

.select-files-li ul li {
    position: relative;
}

.select-files-li ul li span.delete {
    position: absolute;
    right: 0px;
    top: 16px;
    right: 14px;
    cursor: pointer;
}

.select-files-li ul li.active label {
    background-color: #5272b014!important;
}

.select-files-li ul li label img {
    width: 7px;
}

button.btn-secondary {
    background: #5272b014!important;
    /* opacity: 1; */
