body {
    margin: 0px!important;
}

.sign-in {
    padding: 0px !important;
}

.sign-in .row {
    margin: 0px;
}

.sign-in .row .img {
    padding: 0px;
    background: #5272af;
    display: flex;
    align-items: center;
    justify-content: center;
}

.sign-in .row .img img {
    width: 45%;
}

