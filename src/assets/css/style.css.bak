    font-family: Space Grotesk;
    font-size: 18px;
    font-weight: 700;
    line-height: 23px;
    letter-spacing: 0.1em;
    text-align: center;
    color: #5272B0;
    text-transform: uppercase;
    padding: 16px 0px;
}

.select-files-li ul li label {
    border-bottom: #cccccc47 solid 1px;
}

select.form-control,
.form-control {
    background: #5272b014!important;
    border: none;
    border-radius: 0px;
    font-family: Space Grotesk;
    font-size: 16px;
    font-weight: 700;
    line-height: 20px;
    width: 100%;
    padding: 14px 15px;
    letter-spacing: 0.1em;
