select,
input {
    outline: none;
    box-shadow: 0px 0px 0px 0px;
}

.sign-in .form p.forgot-passowrd a {
    font-weight: 600;
}


/*toggle css code*/

.gender-qu .switch {
    position: relative;
    display: inline-block!important;
    width: 60px!important;
    height: 34px;
}

.gender-qu .switch input {
    opacity: 0;
    width: 0;
    height: 0;
}

.gender-qu .switch .slider {
    position: absolute;
    cursor: pointer;
    top: 0;
    left: 0;
