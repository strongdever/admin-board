    font-family: Space Grotesk;
    font-size: 16px;
    font-weight: 700;
    line-height: 20px;
    height: 58px;
    width: 100%;
    padding: 13px 79px;
    letter-spacing: 0.1em;
    text-align: left;
}

.sign-in .form .icons i.show {
    left: inherit;
    right: 0px;
    cursor: pointer;
    border: none;
}

.sign-in .form .icons {
    position: relative;
    margin-bottom: 28px;
}

.sign-in .form .icons i {
    position: absolute;
    left: 0px;
    bottom: 0px;
    border-right: 2px solid #ccc;
    padding: 12px 19px;
    height: 57px;
    display: flex;
    align-items: center;
}

.sign-in .form .icons i img {
    width: 18px;
}

.sign-in .form .main {
    width: 100%;
    padding: 60px 46px;
}

.sign-in .form p.forgot-passowrd {
    font-family: Space Grotesk;
    font-size: 18px;
    font-weight: 700;
    line-height: 23px;
    letter-spacing: 0.2em;
    text-align: right;
    color: #5272B0;
}

.sign-in .form p.forgot-passowrd a {
    text-decoration: underline;
}

.sign-in .form button {
    font-family: Space Grotesk;
    font-size: 24px;
    font-weight: 700;
    line-height: 31px;
    letter-spacing: 0.1em;
    text-align: center;
    background: #5272B0;
    border: none;
    color: #fff;
    padding: 20px 88px;
    float: right;
    margin-top: 33px;
}

.sign-in .forms-app {
    contain: content;
}

.main.shadows label.profile-select {
    position: relative;
}

.main.shadows label.profile-select input[type='file'] {
    position: absolute;
    left: 0px;
    right: 0px;
    bottom: 0px;
    top: 0px;
    height: 100%;
    opacity: 0;
}

.main.shadows label.profile-select i.value {
    position: absolute;
    bottom: -23px;
    font-size: 11px;
    left: 17px;
