    color: #5272B0;
    border-bottom: 8px solid #5272b057;
    padding: 0px 0px 10px;
    margin-bottom: 40px;
}

.sign-in .form p {
    font-family: Space Grotesk;
    font-size: 24px;
    font-weight: 500;
    line-height: 31px;
    letter-spacing: -0.05em;
    text-align: left;
    color: #4C4C4C;
    opacity: 50%;
    padding-bottom: 49px;
}

.sign-in .form label {
    font-family: Space Grotesk;
    font-size: 18px;
    font-weight: 700;
    line-height: 23px;
    letter-spacing: 0.1em;
    color: #5272B0;
    text-align: left;
    /* opacity: 50%; */
    display: block;
    margin-bottom: 11px;
}

.sign-in .form input[type="text"],
.sign-in .form select,
.sign-in .form input[type="password"],
.sign-in .form input[type="email"] {
    background: #5272b014!important;
