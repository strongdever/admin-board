    display: flex;
    align-items: center;
    max-width: 50%;
    width: 50%;
}

.sign-in .form h2 {
    font-family: Space Grotesk;
    font-size: 32px;
    font-weight: 700;
    line-height: 41px;
    letter-spacing: 0.1em;
